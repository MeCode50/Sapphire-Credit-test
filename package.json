  "scripts": {
    "build": "npx prisma db push && npx prisma generate && npx ts-node prisma/seed.ts && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "prisma:dev": "npx prisma migrate dev && npx prisma generate && npx prisma studio",
    "prisma:prod": "npx prisma db pull && npx prisma migrate deploy && npx prisma generate && npx prisma studio",
    "prisma:clear": "npx ts-node prisma/clearMbe.ts",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed": "npx ts-node prisma/seed.ts",
    "import:store": "npx ts-node src/script/import.store.ts",
    "import:company": "npx ts-node src/script/import.company.ts",
    "import:region": "npx ts-node src/script/import.region.ts",
    "import:cluster": "npx ts-node src/script/import.cluster.ts",
    "import:mbe": "npx ts-node src/script/import.mbe.ts"
  },
